#rotate a number
n = input()
k = int(input())
l = list(n)
if k > 0:
    for i in range(k):
        a = l.pop()
        l.insert(0,a)
    for j in l:
        print(j,end='')
elif k < 0:
    for i in range(-k):
        b = l.pop(0)
        l.append(b)
    for j in l:
        print(j,end='')
else:
    print(n)
